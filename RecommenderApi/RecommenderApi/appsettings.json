{
  "UrConfiguration": {
    "EngineId": "gift-recommender",
    "HarnessUrl": "http://harness:9090"
  },
  "GiftRecommenderDatabase": {
    "ConnectionString": "mongodb://mongo:27017",
    "DatabaseName": "gift-recommender",
    "CollectionName": "events"
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.File", "Serilog.Sinks.Console" ],
    "MinimumLevel": {
      "Default": "Debug"
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "./logs/webapi-.log",
          "rollingInterval": "Day",
          "outputTemplate": "[{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} {CorrelationId} {Level:u3}] {Username} {Message:lj}{NewLine}{Exception}"
        }
      },
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} {CorrelationId} {Level:u3}] {Username} {Message:lj}{NewLine}{Exception}"
        }
      }
    ]
  },
  "AllowedHosts": "backend.gift4me.net;https://backend.gift4me.net/harness;https://www.gift4me.net"
}